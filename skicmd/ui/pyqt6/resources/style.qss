/* Default PyQt6 stylesheet */

QMainWindow {
    background-color: #f0f0f0;
}

QPushButton {
    padding: 6px 16px;
    border: 1px solid #ccc;
    border-radius: 4px;
    background-color: #ffffff;
}

QPushButton:hover {
    background-color: #e6e6e6;
}

QPushButton:pressed {
    background-color: #d0d0d0;
}
